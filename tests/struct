unpack struct Vector2
  int x
  int y
end

struct Vector3
  int x
  int y
  int z
end

struct (Vector2) Vector4
  int z
  int w
end

proc [Vector2] 3diffy -> Vector3:
  // I can use unpacking here, but i want to test getting fields
  bind self:
    self .x
    self .y
  end
  0 Vector3
end

69 42 Vector2
bind vec:
  vec @ 
  swap print print
  vec .3diffy
  dup .x print
  dup .y print
  dup .z print
  3 4 vec upcast(Vector4)
  dup .z print
  dup .w print 
end
:
69
42
69
42
0
3
4
