{
  "name": "cont-porgramming-language",
  "displayName": "Cont Porgramming Language",
  "description": "Cont Programming Language Support",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.78.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "cont",
      "aliases": ["Cont", "cont"],
      "extensions": [".cn"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "cont",
      "scopeName": "source.cont",
      "path": "./syntaxes/cont.tmLanguage.json"
    }],
    "commands": [
      {
        "command": "cont.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.81.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
